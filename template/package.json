{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "ts": "tsc -p .",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-reset-cache": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "android:run": "node node_modules/react-native/local-cli/cli.js run-android --no-packager",
    "android:run+start": "node node_modules/react-native/local-cli/cli.js run-android --no-packager && node node_modules/react-native/local-cli/cli.js start",
    "android:build-release-bundle": "(cd android/ && ./gradlew bundleRelease)",
    "android:build-release-apk": "(cd android/ && ./gradlew assembleRelease)",
    "android:clean": "(cd android/ && ./gradlew clean)",
    "android:bundle": "node node_modules/react-native/local-cli/cli.js bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/ --dev false --sourcemap-sources-root=build",
    "android:bundle-visualizer": "react-native-bundle-visualizer --platform android",
    "ios:run": "node node_modules/react-native/local-cli/cli.js run-ios --no-packager",
    "ios:bundle-visualizer": "react-native-bundle-visualizer --platform ios",
    "check-dependencies-updates": "npm outdated",
    "check-pods-updates": "cd ios && pod outdated",
    "tests": "jest",
    "tests:watch": "jest --watchAll",
    "eslint:run": "eslint --ext=jsx,ts,tsx",
    "eslint:fix": "eslint --fix --ext=jsx,ts,tsx ."
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/datetimepicker": "3.5.2",
    "@reduxjs/toolkit": "1.6.1",
    "faker": "5.5.3",
    "intl": "1.2.5",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "react": "16.14.0",
    "react-native": "0.64.2",
    "react-native-device-detection": "0.2.1",
    "react-native-fast-image": "8.3.6",
    "react-native-gesture-handler": "1.10.3",
    "react-native-image-crop-picker": "0.36.2",
    "react-native-localization": "2.1.7",
    "react-native-masked-text": "1.13.0",
    "react-native-navigation": "7.18.0",
    "react-native-navigation-hooks": "6.3.0",
    "react-native-orientation-locker": "1.3.1",
    "react-native-permissions": "3.0.5",
    "react-native-snap-carousel": "3.9.1",
    "react-redux": "7.2.4",
    "redux-persist": "6.0.0",
    "redux-persist-filesystem-storage": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/runtime": "7.14.6",
    "@storybook/addon-actions": "6.3.4",
    "@storybook/addon-knobs": "6.3.0",
    "@storybook/addon-ondevice-actions": "5.3.23",
    "@storybook/addon-ondevice-knobs": "5.3.25",
    "@storybook/react-native": "5.3.25",
    "@types/faker": "5.5.7",
    "@types/intl": "1.2.0",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.171",
    "@types/react": "16.14.11",
    "@types/react-native": "0.64.12",
    "@types/react-native-snap-carousel": "3.8.4",
    "@types/react-redux": "7.1.18",
    "@types/react-test-renderer": "16.9.5",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "babel-jest": "27.0.6",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "7.31.0",
    "eslint-config-react-strong": "0.15.0-alpha",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-native": "3.11.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.0.1",
    "metro-react-native-babel-preset": "0.66.1",
    "patch-package": "6.4.7",
    "prettier": "2.3.2",
    "react-native-bundle-visualizer": "2.2.1",
    "react-test-renderer": "16.14.0",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  },
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "npm run ts && npm run eslint:run"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run ts",
      "npm run eslint:run"
    ]
  },
  "engines": {
    "npm": ">= 6.14.13",
    "yarn": "please-use-npm"
  }
}
